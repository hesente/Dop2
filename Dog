// Дочерний класс Собака
class Dog extends Animal {
    private String size;

    public Dog(String name, int age, String size) {
        super(name, age);
        this.size = size;
    }

    public String getSize() {
        return size;
    }

    @Override
    public void speak() {
        System.out.println(getName() + " лает.");
    }
}
